library(ggplot2)
TicketStatus=c('Solved', 'Open','Reopened','Deleted','Unassigned','Escalated')
Percentage=c(45.5, 39.5, 6.5, 2.6, 2.0, 3.9)
df <- paste(TicketStatus,Percentage)
df <- data.frame(TicketStatus,Percentage)
print(df)
ggplot(df, aes(x = TicketStatus, y = Percentage)) +
geom_segment(aes(x = TicketStatus, xend = TicketStatus, y = 0, yend = Percentage),
color = "gray", lwd = 1) +
geom_point(size = 7.5, pch = 21, bg = 4, col = 1) +
geom_text(aes(label = Percentage), color = "white", size = 3) +
scale_x_discrete(labels = TicketStatus) +
coord_flip() +
theme_minimal()
